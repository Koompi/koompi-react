{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "semi": 0,
    "no-plusplus": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".jsx", ".js"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}
